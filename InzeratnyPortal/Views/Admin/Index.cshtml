@model IList<Microsoft.AspNetCore.Identity.IdentityUser>

@{
    Layout = "_adminLayout";
}



@*<a class="nav-link text-dark" asp-area="Identity" asp-page="/Account/Register">Register</a>*@

@if (Model.Any())
{

    foreach (var user in Model)
    {
        <table class="table table-striped table-dark">
            <thead>
                <tr class="in_table">
                    <th scope="col">ID</th>
                   
                    <th scope="col">E-mail</th>
                    
                    <th scope="col"></th>
                </tr>
            </thead>
            <tbody>
                <tr class="in_table">
                    <th scope="row">@user.Id</th>
                   
                    <td>@user.Email</td>
                    
                    <td>
                        <div class="ikonky">
                            <a asp-area="Identity" asp-page="/Account/Manage">
                                <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-presentation" width="40" height="40" viewBox="0 0 24 24" stroke-width="1" stroke="#e6e6e6" fill="none" stroke-linecap="round" stroke-linejoin="round">
                                    <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                                    <line x1="3" y1="4" x2="21" y2="4" />
                                    <path d="M4 4v10a2 2 0 0 0 2 2h12a2 2 0 0 0 2 -2v-10" />
                                    <line x1="12" y1="16" x2="12" y2="20" />
                                    <line x1="9" y1="20" x2="15" y2="20" />
                                    <path d="M8 12l3 -3l2 2l3 -3" />
                                </svg>
                            </a>
                            <a asp-area="Identity" asp-page="/Account/Manage/Index">
                                <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-edit" width="40" height="40" viewBox="0 0 24 24" stroke-width="1" stroke="#e6e6e6" fill="none" stroke-linecap="round" stroke-linejoin="round">
                                    <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                                    <path d="M9 7h-3a2 2 0 0 0 -2 2v9a2 2 0 0 0 2 2h9a2 2 0 0 0 2 -2v-3" />
                                    <path d="M9 15h3l8.5 -8.5a1.5 1.5 0 0 0 -3 -3l-8.5 8.5v3" />
                                    <line x1="16" y1="5" x2="19" y2="8" />
                                </svg>
                            </a>
                            <a asp-action="DeleteUser" asp-controller="Admin" asp-route-id="@user.Id">
                                <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-trash" width="40" height="40" viewBox="0 0 24 24" stroke-width="1" stroke="#e6e6e6" fill="none" stroke-linecap="round" stroke-linejoin="round">
                                    <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                                    <line x1="4" y1="7" x2="20" y2="7" />
                                    <line x1="10" y1="11" x2="10" y2="17" />
                                    <line x1="14" y1="11" x2="14" y2="17" />
                                    <path d="M5 7l1 12a2 2 0 0 0 2 2h8a2 2 0 0 0 2 -2l1 -12" />
                                    <path d="M9 7v-3a1 1 0 0 1 1 -1h4a1 1 0 0 1 1 1v3" />
                                </svg>
                            </a>
                        </div>
                    </td>
                </tr>
            </tbody>
        </table>
    }



}
else
{


}
